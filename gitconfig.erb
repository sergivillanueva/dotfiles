[user]
	name = Enric Lluelles
	email = enriclluelles@gmail.com
[alias]
	co = checkout
  msg = commit --allow-empty -m
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = /Users/enric/.gitignore
  editor = vim
[apply]
	whitespace = nowarn
[pull]
	default = tracking
[push]
	default = tracking
[github]
  user = enriclluelles
  token = <%= print("GitHub API Token: "); STDOUT.flush; STDIN.gets.chomp %>
[branch]
	autosetuprebase = always
[merge]
  tool = custom
[mergetool "custom"]
  cmd=$EDITOR -d -g $LOCAL $MERGED $REMOTE
  keepbackup=false
